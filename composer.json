    "require": {
        "php": "^7.3",
        "nesbot/carbon": "^2.16",
        "ext-json": "*",
        "php-http/message": "^1.8",
        "guzzlehttp/psr7": "^1.6",
        "php-http/guzzle6-adapter": "^1.0 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "squizlabs/php_codesniffer": "^3.0",
        "phpfastcache/phpfastcache": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "TruckersMP\\APIClient\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit",
            "./vendor/bin/phpcs src --standard=PSR12"
        ]
    }
}
